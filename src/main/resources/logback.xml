<?xml version="1.0" encoding="utf-8" ?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %-5level %logger{0}: %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ua.home.web" level="INFO"/>

    <root level="error">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>